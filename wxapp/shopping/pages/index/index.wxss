.swiper{
height: 421rpx;
}

.slide-image{
  width: 100%;
  height: 100%;
}
.selected{

}
.select-title,
.newest-title{
text-align: center;
color: #ab956d;
padding:30rpx;
}
.select-top,
.select-bottom{
  width: 100%;
  height: 375rpx;
}
.select-top__small{
  width: 375rpx;
  height: 100%;
  float: left;
}
.select-top__small  navigator,
.select-top__small image,
.select-bottom  navigator,
.select-bottom image{
  width: 100%;
  height: 100%;
}
.newest-box{
  padding:0 40rpx;
}
.newest-list{
  background-color: #f5f6f5;
  border-radius: 10px;
  display: inline-block;
  width: 325rpx;
  height: 325rpx;
   margin: 0 20rpx 20rpx 0;
   text-align: center;
}
.newest-list:nth-child(2n){
  margin-right: 0;
}
.newest-list image{
  width: 175rpx;
  height: 175rpx;
  margin: 20rpx 0 10rpx;
}