* {
    -webkit-tap-highlight-color: rgba(255, 0, 0, 0);
}

body {
    margin: 0;
    color: #191919;
    font-size: 12px;
    font-family: 'Helvetica Neue', Helvetica, Roboto, Arial, sans-serif;
    position: relative;
}

body {
    background: #f6f6f6;
}

div,
p,
span,
a {
    margin: 0;
    padding: 0;
}

html body {
    margin: 0;
}

.TalionNav {
    width: 100%;
    position: relative;
    z-index: 9999;
    background: #fff;
}


/* TalionNav-primary部分 */

.TalionNav-primary {
    height: 50px;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 100;
    padding: 0 15px;
    background-color: #fff;
    box-shadow: 0 1px 4px rgba(0, 0, 0, 0.05);
}

input,
textarea,
button,
select,
a {
    -webkit-tap-highlight-color: transparent;
}

a {
    color: #42bd56;
    text-decoration: none;
}

a,
input {
    outline: 0;
}

.TalionNav .logo {
    margin-right: 15px;
}

.TalionNav .logo {
    display: inline-block;
    font-size: 0;
    height: 26px;
    width: 54px;
    flex: 54px 0 0;
    background: url(data:image/png;base64,) 50% no-repeat;
    background-size: contain;
}

h1,
h2,
h3 {
    font-weight: normal;
}

h1 {
    margin: 30px 0 5px;
    font-size: 24px;
    line-height: 32px;
    word-break: break-all;
}

.TalionNav .logo H1 {
    opacity: 0;
}

.TalionNav-primary,
.TalionNav-primary .search-box {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    box-sizing: border-box;
}

.TalionNav-primary .search-box {
    width: 100px;
    font: 100px 0 0;
    height: 30px;
    border-radius: 15px;
    border: 1px solid #dfdfdf;
    color: rgba(0, 0, 0, 0.25);
    background-color: #f7f7f7;
    font-size: 15px;
    line-height: 21;
    padding-left: 8px;
}

input {
    outline: 0;
}

.TalionNav-primary .search-box input[type=search] {
    border-style: border-box;
    -webkit-appearance: none;
    border: 0;
    padding-left: 20px;
    background: url(data:image/png;base64,) no-repeat 0;
    background-size: 17.333333px;
    border-radius: 3px;
    width: 100%;
    height: 31px;
    font-size: 14px;
}

.TalionNav-primary .nav-btns {
    flex: 1;
    font-size: 15px;
    line-height: 21;
    color: #3ba94d;
    text-align: right;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.TalionNav a {
    text-decoration: none;
}

.TalionNav-primary .nav-btns .nav-btn-item {
    padding: 0 15px;
    border-right: 1px solid rgba(0, 0, 0, 0.12);
}

.TalionNav-primary .nav-btns .to_pc {
    display: inline-block;
    width: 20px;
    height: 19px;
    background: url(data:image/png;base64,) 100% no-repeat;
    background-size: contain;
}

.TalionNav-primary .nav-btns .nav-btn-item:first-child {
    padding-left: 0;
}

.TalionNav-primary .nav-btns .nav-btn-item:last-child {
    padding-right: 0;
    border-right: 0;
}


/* TalionNav-sattic 部分*/

.TalionNav-static {
    left: 0;
    width: 100%;
    background-color: #f7f7f7;
    justify-content: flex-start;
    align-items: center;
    margin-top: 50px;
    padding: 0 18px;
    height: 60px;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
}

.TalionNav-static>* {
    word-spacing: nowrap;
}

.TalionNav-static>a {
    font-size: 0;
}

.TalionNav .icon-wrap {
    margin-right: 10px;
}

.TalionNav .icon-wrap .icon {
    width: 39px;
    height: 39px;
}

.TalionNav .info {
    flex: 1;
}

.TalionNav .info h2 {
    font-size: 15px;
    line-height: 21px;
    color: #191919;
    margin: 0;
}

.TalionNav .info p {
    font-size: 11px;
    line-height: 16px;
    color: #818181;
    margin: 0;
}

.TalionNav .btn {
    width: 80px;
    height: 30px;
    border-radius: 15px;
    text-align: center;
    font-size: 13px;
    line-height: 30px;
}

.TalionNav .btn.green {
    background-color: #42bd56;
    color: #ffffff;
}

.TalionNav-static:after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    border: 1px solid #dfdfdf;
    border-left: none;
    border-right: none;
    box-sizing: border-box;
    width: 200%;
    height: 200%;
    transform: scale(0.5) translateX(-0.5px) translateY(-0.5px);
    transform-origin: left top;
    pointer-events: none;
}


/* page */

.page {
    padding-top: 0;
    background-color: #ffffff;
    margin: 0 auto;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
}

body .page,
body #app,
body .msite-max-width,
body .msite-fixed-max-width {
    max-width: 650px;
}

.subject-header-wrap {
    padding: 20px 15px 15px;
    transform: all 0.2s;
}

.sub-info {
    min-height: 165px;
}

.subject-header-wrap .sub-cover {
    float: left;
    position: relative;
    z-index: 1;
}

.subject-header-wrap .sub-cover img {
    width: 100px;
    height: 140px;
    border-radius: 4px;
    border: 1px solid rgba(0, 0, 0, 0.05);
    box-shadow: 0 5px 10px rgba(0, 0, 0, 0.15);
}
*
  margin 0
  padding 0